{
  "author": "jamuhl",
  "name": "i18next-conv",
  "description": "converts .mo and .po files to i18next's json format and vice versa.",
  "keywords": [
    "i18next",
    "gettext"
  ],
  "version": "2.6.0",
  "private": false,
  "preferGlobal": "true",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/i18next/i18next-gettext-converter.git"
  },
  "engines": {
    "node": ">= 0.6.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.5.0",
    "gettext-parser": "^1.1.2",
    "mkdirp": "^0.5.0",
    "node-gettext": "^1.0.1"
  },
  "devDependencies": {
    "async": "0.9.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "chai": "1.10.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "mocha": "2.4.5"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "pretest": "npm run lint",
    "test": "mocha",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build"
  },
  "bin": {
    "i18next-conv": "program.js"
  }
}
